{
  "presets": [
    "env",
    "flow",
    "react",
    "stage-2"
  ],
  "env": {
    "test": {
      "plugins": [
        "transform-es2015-modules-commonjs"
      ]
    },
    "development": {
      "plugins": [
        "flow-react-proptypes"
      ]
    }
  },
  "plugins": [
    [
      "import",
      {
        "libraryName": "antd",
        "style": "css"
      }
    ]
  ]
}