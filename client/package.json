    "lint": "yarn lint:flow && yarn lint:eslint && yarn lint:prettier",
    "lint:build": "yarn lint:flow && yarn lint:eslint",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "start": "yarn storybook"
  },
  "cacheDirectories": [
